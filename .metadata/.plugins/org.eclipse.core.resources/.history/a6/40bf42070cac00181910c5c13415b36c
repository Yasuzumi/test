package com.internousdev.ecsite.action;

import java.util.Map;

import org.apache.struts2.interceptor.SessionAware;

import com.opensymphony.xwork2.ActionSupport;



public class ItemUserCreateConfirmAction extends ActionSupport implements SessionAware {

	private String itemName;
		private String itemPrice;
		private String itemStock;
		public Map<String,Object>session;
		private String errorMessage;

		public String execute(){

				String result = SUCCESS;
				if(!(itemName.equals(""))
					&& !(itemPrice.equals(""))
					&& !(itemStock.equals(""))) {
						session.put("loginUserId",itemName);
						session.put("loginPassword",itemPrice);
						session.put("userName", itemStock);
				}else{
						setErrorMessage("未入力の項目があります。");
						result = ERROR;



				}
				return result;
			}

		public String getLoginUserId(){
				return itemName;
		}

		public void setLoginUserId(String itemName){
				this.itemName = itemName;
		}

		public String getLoginPassword(){
				return itemPrice;
		}

		public void setLoginPassword(String loginPassword){
				this.itemPrice = loginPassword;
		}

		public String getUserName() {
				return itemStock;
		}

		public void setUserName(String userName){
				this.itemStock = userName;
		}

		public Map<String, Object> getSession(){
				return session;
		}

		@Override
		public void setSession(Map<String, Object> session){
				this.session = session;
		}

		public String getErrorMessage(){
				return errorMessage;
		}

		public void setErrorMessage(String errorMessage){
				this.errorMessage = errorMessage;

		}
}
